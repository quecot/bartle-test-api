{
  "name": "bartle-test-api",
  "version": "0.0.0",
  "type": "module",
  "main": "index.js",
  "license": "MIT",
  "engines": {
    "node": "^16.0.0 || >=18.0.0"
  },
  "scripts": {
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "start": "node dist/index.js",
    "lint": "eslint --ext .ts .",
    "dev": "nodemon --watch 'src/**' --ext 'ts,json' --ignore 'src/**/*.spec.ts' --exec 'tsc --project tsconfig.json && tsc-alias -p tsconfig.json && node dist/index.js'"
  },
  "dependencies": {
    "body-parser": "1.20.2",
    "cohere-ai": "6.2.2",
    "cors": "2.8.5",
    "dotenv": "16.3.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "express": "4.19.2",
    "mongoose": "8.0.0"
  },
  "devDependencies": {
    "@types/body-parser": "1.19.4",
    "@types/cors": "2.8.15",
    "@types/express": "4.17.20",
    "@types/node": "20.8.10",
    "@typescript-eslint/eslint-plugin": "6.9.1",
    "@typescript-eslint/parser": "6.9.1",
    "eslint": "8.52.0",
    "eslint-config-airbnb-typescript-base": "4.0.2",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-import": "2.29.0",
    "nodemon": "3.0.1",
    "ts-node": "10.9.1",
    "tsc-alias": "1.8.8",
    "typescript": "5.2.2",
    "webpack": "5.89.0"
  }
