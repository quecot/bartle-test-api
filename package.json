{
  "name": "bartle-test-api",
  "version": "0.0.0",
  "type": "module",
  "main": "index.js",
  "license": "MIT",
  "engines": {
    "node": "14.18.0"
  },
  "scripts": {
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "start": "node dist/index.js",
    "lint": "eslint --ext .ts .",
    "dev": "nodemon --watch 'src/**' --ext 'ts,json' --ignore 'src/**/*.spec.ts' --exec 'tsc --project tsconfig.json && tsc-alias -p tsconfig.json && node dist/index.js'"
  },
  "dependencies": {
    "body-parser": "^1.20.1",
    "cohere-ai": "5.0.2",
    "cors": "2.8.5",
    "dotenv": "16.0.3",
    "eslint-plugin-react": "7.32.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "express": "4.18.2",
    "mongoose": "6.9.0"
  },
  "devDependencies": {
    "@types/cors": "2.8.13",
    "@types/express": "4.17.16",
    "@types/node": "18.11.18",
    "@typescript-eslint/eslint-plugin": "5.49.0",
    "@typescript-eslint/parser": "5.49.0",
    "eslint": "8.32.0",
    "eslint-config-airbnb-typescript-base": "4.0.2",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-import": "2.27.5",
    "ts-node": "10.9.1",
    "tsc-alias": "1.8.2",
    "typescript": "4.9.4",
    "webpack": "5.75.0"
  }
}
